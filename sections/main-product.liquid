                            {%- for value in option.values -%}
                            <input type="radio" id="{{ section.id }}-{{ option.position }}-{{ forloop.index0 }}"
                                   name="{{ option.name }}"
                                   value="{{ value | escape }}"
                                   form="{{ product_form_id }}"
                                    sku="{{ product.variants[forloop.index0].sku }}"
                                   {% if option.selected_value == value %}checked{% endif %}
                                   >
                              
                            <label names="{{ option.name }}" class="tooltip"  tol_variant_id="{{ product.variants[forloop.index0].id }}"  tol_variant_quant="{{ product.variants[forloop.index0].inventory_quantity }}" for="{{ section.id }}-{{ option.position }}-{{ forloop.index0 }}">
