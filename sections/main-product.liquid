                  {%- for option in product.options_with_values -%}
                      <fieldset class="js product-form__input product-variant-design" data-parent="{{ option.name | escape }}">
                      <div class="size-sectio">
                          <div class="size-left">
                            <legend class="form__label">{{ option.name }}</legend>
                          </div>
